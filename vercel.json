{
  "buildCommand": "npm run build",
  "installCommand": "npm ci --prefer-offline --no-audit",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
